{
  "name": "app-store-scraper-ts",
  "version": "0.1.0",
  "description": "TypeScript rewrite of app-store-scraper with throttling, retries, and strong typing.",
  "keywords": [
    "app-store",
    "itunes",
    "scraper",
    "typescript"
  ],
  "homepage": "https://github.com/Guiguito/app-store-scraper-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guiguito/app-store-scraper-ts.git"
  },
  "bugs": {
    "url": "https://github.com/Guiguito/app-store-scraper-ts/issues"
  },
  "author": "Guiguito",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "node node_modules/typescript/bin/tsc -p tsconfig.esm.json",
    "build:cjs": "node node_modules/typescript/bin/tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "npm run build:cjs && node node_modules/mocha/bin/mocha \"tests/*.spec.js\" \"tests/**/*.spec.js\"",
    "test:coverage": "npm run build:cjs && node node_modules/nyc/bin/nyc.js node node_modules/mocha/bin/mocha \"tests/*.spec.js\" \"tests/**/*.spec.js\"",
    "docs": "typedoc",
    "ui": "npm run build:cjs && node ui-test/server.js",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "fast-xml-parser": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "chai": "^4.4.1",
    "eslint": "^9.9.0",
    "mocha": "^10.7.0",
    "nock": "^13.5.0",
    "prettier": "^3.3.3",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "undici": "^6.19.8"
  }
}
